{# -- DBMS Name          : SNOWFLAKE                                                                                                                        -- #}
{# -- Template Name      : wsl_snowflake_pscript_dv_perm                                                                                                    -- #}
{# -- RED Version        : 8.3.1.0                                                                                                                          -- #}
{# -- Description        : This template creates a SNOWFLAKE procedure using Insert                                                                         -- #}
{# --                      specifically designed for RED data vault permanent tables                                                                        -- #}
{# --                                                                                                                                                       -- #}
{# -- =============================================================================                                                                         -- #}
